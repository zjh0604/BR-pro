# 后端环境配置
BACKEND_ENVIRONMENT=test  # test, pre, prod

# 后端Redis配置（共享后端Redis服务）
BACKEND_REDIS_HOST=backend-server
BACKEND_REDIS_PORT=6379
BACKEND_REDIS_DB=10
BACKEND_REDIS_PASSWORD=your_backend_redis_password

# 后端API配置
BACKEND_API_URL=http://backend-server/api
BACKEND_API_TIMEOUT=30

# Milvus向量数据库配置
MILVUS_HOST=localhost
MILVUS_PORT=19530

# 安全配置
AES_KEY=your_aes_key_32_chars_long
HMAC_KEY=your_hmac_key_32_chars_long

# 服务配置
LOG_LEVEL=INFO
ENVIRONMENT=production
